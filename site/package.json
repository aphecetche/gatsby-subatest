{
  "name": "site",
  "private": true,
  "description": "Site web pour le laboratoire Subatech, Nantes, France",
  "version": "0.1.0",
  "author": "Laurent Aphecetche",
  "dependencies": {
    "gatsby": "^2.28.0",
    "gatsby-theme-article": "*",
    "gatsby-theme-intl": "*",
    "gatsby-theme-ldap": "*",
    "gatsby-theme-mui": "*",
    "gatsby-theme-nav": "*",
    "gatsby-theme-seminar": "*",
    "gatsby-theme-orgchart": "*"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aphecetche/gatsby-subatest",
    "directory": "packages/site"
  },
  "scripts": {
    "start": "touch .env && env-cmd -f .env gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
